<%= nested_form_for [:admin, @business], :html => { :class => "form-horizontal business" } do |f| %>

  <%= render partial: 'step1', locals: { f: f } %>
  <%= f.submit 'Create' ,:class => 'btn btn-primary col-sm-offset-4' %>

  <%= link_to t('.cancel', :default => t("helpers.links.cancel")),
      admin_businesses_path, :class => 'btn btn-warning' %>

<% end %>
<% content_for :head do javascript_include_tag :businesses end %>
